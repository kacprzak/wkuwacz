<assembly>
    <id>linux32</id>
    <formats>
        <format>tar.gz</format>
    </formats>
    <dependencySets>
        <dependencySet>
            <outputDirectory>lib</outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <includes>
                <include>entertech.swt:swt-gtk-linux-x86</include>
            </includes>
        </dependencySet>
        <dependencySet>
            <outputDirectory></outputDirectory>
            <unpack>false</unpack>
            <scope>runtime</scope>
            <includes>
                <include>pl.kacprzak:wkuwacz</include>
            </includes>
        </dependencySet>
    </dependencySets>
    <fileSets>
        <fileSet>
            <directory>target/files</directory>
            <outputDirectory></outputDirectory>
            <includes>
                <include>**/*.txt</include>
            </includes>
        </fileSet>
    </fileSets>
    <files>
        <file>
            <source>target/files/wkuwacz.sh</source>
            <outputDirectory></outputDirectory>
            <destName>wkuwacz.sh</destName>
            <fileMode>0755</fileMode>
        </file>
    </files>
</assembly>

